From b52cac64b03098251895eb9d69c93aaa6e5519c1 Mon Sep 17 00:00:00 2001
From: Philippe Bellanger <pbellanger@hotmail.com>
Date: Thu, 30 Mar 2017 20:53:43 -0400
Subject: [PATCH] Included additional overrides to remove unnecessary aosp
 packages

---
 modules/GoogleExtServices/Android.mk                | 3 +++
 modules/GoogleExtShared/Android.mk                  | 3 +++
 modules/GoogleOneTimeInitializer/Android.mk         | 3 +++
 modules/GooglePrintRecommendationService/Android.mk | 3 +++
 4 files changed, 12 insertions(+)

diff --git a/modules/GoogleExtServices/Android.mk b/modules/GoogleExtServices/Android.mk
index 4bf3c45..b565edb 100644
--- a/modules/GoogleExtServices/Android.mk
+++ b/modules/GoogleExtServices/Android.mk
@@ -5,4 +5,7 @@ LOCAL_MODULE := GoogleExtServices
 LOCAL_PACKAGE_NAME := com.google.android.ext.services
 LOCAL_PRIVILEGED_MODULE := true
 
+GAPPS_LOCAL_OVERRIDES_MIN_VARIANT :=
+GAPPS_LOCAL_OVERRIDES_PACKAGES := ExtServices
+
 include $(BUILD_GAPPS_PREBUILT_APK)
diff --git a/modules/GoogleExtShared/Android.mk b/modules/GoogleExtShared/Android.mk
index 3257346..4df9da4 100644
--- a/modules/GoogleExtShared/Android.mk
+++ b/modules/GoogleExtShared/Android.mk
@@ -4,4 +4,7 @@ include $(GAPPS_CLEAR_VARS)
 LOCAL_MODULE := GoogleExtShared
 LOCAL_PACKAGE_NAME := com.google.android.ext.shared
 
+GAPPS_LOCAL_OVERRIDES_MIN_VARIANT :=
+GAPPS_LOCAL_OVERRIDES_PACKAGES := ExtShared
+
 include $(BUILD_GAPPS_PREBUILT_APK)
diff --git a/modules/GoogleOneTimeInitializer/Android.mk b/modules/GoogleOneTimeInitializer/Android.mk
index ef718e2..944582d 100644
--- a/modules/GoogleOneTimeInitializer/Android.mk
+++ b/modules/GoogleOneTimeInitializer/Android.mk
@@ -5,4 +5,7 @@ LOCAL_MODULE := GoogleOneTimeInitializer
 LOCAL_PACKAGE_NAME := com.google.android.onetimeinitializer
 LOCAL_PRIVILEGED_MODULE := true
 
+GAPPS_LOCAL_OVERRIDES_MIN_VARIANT :=
+GAPPS_LOCAL_OVERRIDES_PACKAGES := OneTimeInitializer
+
 include $(BUILD_GAPPS_PREBUILT_APK)
diff --git a/modules/GooglePrintRecommendationService/Android.mk b/modules/GooglePrintRecommendationService/Android.mk
index 813670d..d078019 100644
--- a/modules/GooglePrintRecommendationService/Android.mk
+++ b/modules/GooglePrintRecommendationService/Android.mk
@@ -4,4 +4,7 @@ include $(GAPPS_CLEAR_VARS)
 LOCAL_MODULE := GooglePrintRecommendationService
 LOCAL_PACKAGE_NAME := com.google.android.printservice.recommendation
 
+GAPPS_LOCAL_OVERRIDES_MIN_VARIANT :=
+GAPPS_LOCAL_OVERRIDES_PACKAGES := PrintRecommendationService
+
 include $(BUILD_GAPPS_PREBUILT_APK)
-- 
